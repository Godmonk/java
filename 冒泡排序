package T7;

public class Demon2 {
	public static void main(String[] args){
int [] i={4,-1,1,18,5,0,-5,-6,3};
for(int j=0;j<i.length-1;j++){
	for(int k=0;k<i.length-1-j;k++){
		if(i[k]<i[k+1]){
			int temp=i[k];
			i[k]=i[k+1];
			i[k+1]=temp;
		}
	}
}
for(int y=0;y<i.length;y++){
	System.out.print(i[y]+" ");
}
}
}
